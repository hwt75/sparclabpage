{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"],
}
